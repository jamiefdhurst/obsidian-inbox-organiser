{
  "name": "obsidian-inbox-organiser",
  "version": "0.2.2",
  "description": "Capture any new notes into an inbox and periodically prompt to organise these into other folders within the vault.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "test": "sh fix-jest-environment-obsidian.sh && jest",
    "coverage": "sh fix-jest-environment-obsidian.sh && jest --coverage",
    "test-ci": "sh fix-jest-environment-obsidian.sh && jest --ci --reporters=default --reporters=jest-junit --coverage",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "husky",
    "version": "node version-bump.mjs"
  },
  "keywords": [],
  "author": "Jamie Hurst <jamie@jamiehurst.co.uk>",
  "bugs": {
    "url": "https://github.com/jamiefdhurst/obsidian-inbox-organiser/issues"
  },
  "homepage": "https://github.com/jamiefdhurst/obsidian-inbox-organiser#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.27.2",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-environment-obsidian": "^0.0.1",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.2.7",
    "obsidian": "^1.6.6",
    "obsidian-daily-notes-interface": "^0.9.4",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{js,json,md}": [
      "prettier --write"
    ]
  }
}
